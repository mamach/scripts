@REM type the file name in terminal to execute
doskey ls=dir
doskey ll=dir /w
doskey cdrepos=cd %userprofile%/Desktop/Projects/repos
doskey cdscripts=cd %userprofile%/Desktop/Projects/scripts
doskey pwd=cd
doskey lsdoskey=doskey /macros
doskey cat=type $*
doskey nano=notepad $*
doskey touch=echo > $*
doskey grep=findstr $*
doskey reboot=shutdown /r
doskey rm=del $*
doskey cp=copy $*
doskey mv=move $*